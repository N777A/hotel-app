
    <table>
        <tr>
            <th>施設名</th>
            <th>紹介文</th>
            <th>料金</th>
            <th>作成日</th>
        </tr>
        <% @rooms.each do |room| %>
        <tr>
            <td><%= room.name %></td>
            <td><%= room.intro %></td>
            <td><%= room.fee %></td>
            <td><%= room.created_at.strftime("%Y/%m/%d") %></td>
        </tr>
        <% end %>
    </table>

